          <!-- <img src="<%= productList[0].Img.contentType%>;base64,
                <%=productList[0].Img.data.toString('base64')%>"> -->
          <% for(var i=0; i < productList.length; i++) { %>
            <div class="card">
              <a href="<%= '/info/' + productList[i].Name %>">
                <img
                  src="data:image/<%=productList[i].Img.contentType%>;base64, <%=productList[i].Img.data.toString('base64')%>"
                  class="card-img-top" alt="product images">
                <div class="overlay">
                  <%= productList[i].Name %>
                </div>
              </a>
            </div>
            <% } %>
              <button type="button" class="btn btn-light" id="nextBtn"><i class="fas fa-chevron-right"></i></button>
        </div>
      </div>
      <div class="wrap-scroll-list">
        <div class="head">
