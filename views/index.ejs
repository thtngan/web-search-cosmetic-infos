        <label for="search-box"><span class="fas fa-search search-icon"></span></label>
        <input type="submit" id="search-submit" />
      </form>
      <div class="d-flex p-2" id="myDiv"></div>
      <div class="head">
        <h1>Khám phá</h1>
      </div>
      <div id="discovery-carousel" class="carousel carousel-dark slide" data-bs-ride="carousel">
        <div class="carousel-indicators">
